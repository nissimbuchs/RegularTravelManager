{
  "name": "@rtm/infrastructure",
  "version": "1.0.0",
  "private": true,
  "description": "AWS CDK Infrastructure as Code",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "SKIP_INTEGRATION=false vitest run test/infrastructure-integration.test.ts",
    "test:all": "vitest run && npm run test:integration",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "diff": "cdk diff",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.100.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.100.0-alpha.0",
    "aws-cdk-lib": "^2.100.0",
    "bootstrap": "^5.3.8",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "^3.879.0",
    "@aws-sdk/client-cloudformation": "^3.879.0",
    "@aws-sdk/client-cloudwatch": "^3.879.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.879.0",
    "@aws-sdk/client-location": "^3.879.0",
    "@aws-sdk/client-rds": "^3.879.0",
    "@aws-sdk/client-sns": "^3.879.0",
    "@aws-sdk/client-ssm": "^3.879.0",
    "@types/node": "^18.18.0",
    "aws-cdk": "^2.100.0",
    "ts-node": "^10.9.0",
    "vitest": "^1.0.0"
  }
}
